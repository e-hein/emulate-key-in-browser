{
  "name": "emulate-key-in-browser-in-plain-html",
  "version": "0.0.0",
  "description": "test emulate-key-in-browser in plain html",
  "scripts": {
    "test": "karma start",
    "pretest:once": "npm run build",
    "test:once": "karma start --browsers=ChromeHeadless,FirefoxHeadless --single-run",
    "build": "DEBUG=demo-build* node build.js",
    "build_watch": "npm-watch build",
    "prestart": "npm run build",
    "start": "http-server www"
  },
  "watch": {
    "build": {
      "patterns": [
        "../../dist/bundles",
        "build.js",
        "../assets"
      ],
      "extensions": "*",
      "quiet": false,
      "inherit": false,
      "verbose": true
    }
  },
  "author": "Emanuel Hein",
  "license": "MIT",
  "devDependencies": {
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "jasmine": "^3.5.0",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-firefox-launcher": "^1.3.0",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-jasmine": "^3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-mocha-reporter": "^2.2.5",
    "npm-watch": "^0.6.0",
    "shelljs": "^0.8.4"
  },
  "dependencies": {}
}
